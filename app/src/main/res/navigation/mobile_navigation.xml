<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.myapplication.ui.recipes.RecipesFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_recipes" />
    <fragment
        android:id="@+id/navigation_tocook"
        android:name="com.example.myapplication.ui.toCook.ToCookFragment"
        android:label="@string/title_tocook"
        tools:layout="@layout/fragment_tocook" />
    <fragment
        android:id="@+id/navigation_ingredients"
        android:name="com.example.myapplication.ui.toBuy.ToBuyFragment"
        android:label="@string/title_ingredients"
        tools:layout="@layout/fragment_tobuy" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myapplication.ui.profile.ProfileFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_profile" />



</navigation>